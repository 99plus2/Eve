;*****************************************************
; An incrementer button
;*****************************************************

* increment button
  > event label="increment" | clicks = count(@event.eid)
  ["div"
  ["button" {"click" "increment"}
    "I've been clicked " clicks " times."]
    ["codemirror" {"id" "wah" "foo" "bar"}]
    ]

